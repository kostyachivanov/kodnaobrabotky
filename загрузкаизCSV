Процедура ЗагрузитьПреподователиНаСервере() 
НачальнаяСтрока = 1; 
КонечнаяСтрока = Данные.ВысотаТаблицы; 
Для сч=НачальнаяСтрока По КонечнаяСтрока Цикл 
Преподаватели = Справочники.Преподаватели.СоздатьЭлемент(); 
Код = Данные.Область(сч,1).Текст; 
Преподаватели.Код = Число(Код); 
ФИО = Данные.Область(сч,2).Текст; 
Преподаватели.Наименование = ФИО; 
ФамИмяОтч = СтрЗаменить(ФИО," ",Символы.ПС); 
Преподаватели.Фамилия = СтрПолучитьСтроку(ФамИмяОтч,1); 
Преподаватели.Имя = СтрПолучитьСтроку(ФамИмяОтч,2); 
Преподаватели.Отчество = СтрПолучитьСтроку(ФамИмяОтч,3); 
Должность = Данные.Область(сч,3).Текст; 
Преподаватели.Должность = Справочники.Должности.НайтиПоНаименованию(Должность); 
ДатаРождения = Данные.Область(сч,4).Текст; 
ДатаРожденияГод = Число(Сред(ДатаРождения,7,4)); 
ДатаРожденияМесяц = Число(Сред(ДатаРождения,4,2)); 
ДатаРожденияДень = Число(Лев(ДатаРождения,2)); 
Преподаватели.ДатаРождения = Дата(ДатаРожденияГод,ДатаРожденияМесяц,ДатаРожденияДень); 
Пол = Данные.Область(сч,5).Текст; 
Если Пол="Мужской" Тогда 
Преподаватели.Пол = Перечисления.Пол.М; 
Иначе Преподаватели.Пол = Перечисления.Пол.Ж; 
КонецЕсли; 
Кафедра = Данные.Область(сч,6).Текст; 
Преподаватели.Кафедра = Справочники.Кафедры.НайтиПоНаименованию(Кафедра); 
Преподаватели.Записать(); 
КонецЦикла; 
КонецПроцедуры



&НаСервере 
Процедура ЗагрузитьНаСервере() 
НачальнаяСтрока = 1; 
КонечнаяСтрока = Данные.ВысотаТаблицы; 
Для сч=НачальнаяСтрока По КонечнаяСтрока Цикл 
НовыйСотрудник = Справочники.Сотрудники.СоздатьЭлемент(); 
НовыйСотрудник.Наименование = Данные.Область(сч,3).Текст; //Читаем как текст 
НовыйСотрудник.ФИО = Данные.Область(сч,4).Текст; // Читаем как текст 
ДатаРождения = Данные.Область(сч,5).Текст; // Читаем как текст 
День = Лев(ДатаРождения,2); 
Месяц = Сред(ДатаРождения,4,2); 
Год = Прав(ДатаРождения,4); 
//Собираем Дату 
НовыйСотрудник.ДатаРождения = Дата(Год,Месяц,День); 
ОрганизацияИз = Данные.Область(сч,1).Текст; // Читаем как текст 
Если Справочники.Организации.НайтиПоНаименованию(ОрганизацияИз) = Справочники.Организации.ПустаяСсылка()Тогда 
НоваяОрганизация = Справочники.Организации.СоздатьЭлемент(); 
НоваяОрганизация.Наименование = ОрганизацияИз; 
НоваяОрганизация.Записать(); 
Сообщить("Добавлена организация" + НоваяОрганизация.Наименование); 
КонецЕсли; 
НовыйСотрудник.Организация = Справочники.Организации.НайтиПоНаименованию(ОрганизацияИз); 
ДолжностьИз =Данные.Область(сч,2).Текст; //Читаем как текст 
Если Справочники.Должности.НайтиПоНаименованию(ДолжностьИз) = Справочники.Должности.ПустаяСсылка() Тогда 
НоваяДолжность = Справочники.Должности.СоздатьЭлемент(); 
НоваяДолжность.Наименование = ДолжностьИз; 
НоваяДолжность.Записать(); 
Сообщить("Добавлена должность" + НоваяДолжность.Наименование); 
КонецЕсли; 
НовыйСотрудник.Должность = Справочники.Должности.НайтиПоНаименованию(ДолжностьИз); 
НовыйСОтрудник.Записать(); 
Сообщить("Добавлена должность" + НовыйСотрудник.ФИО); 
КонецЦикла; 
КонецПроцедуры
